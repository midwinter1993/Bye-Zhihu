{
  "name": "Focus",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Good Good Study",
  "browser_action": {
    "default_icon": "focus-off.png",
    "default_title": "focus"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.zhihu.com/*",
        "https://www.zhihu.com/*"
      ],
      "js": ["focus.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
  ]
}
